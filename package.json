{
  "name": "johnwargo",
  "version": "0.1.69",
  "description": "The static site generated version of [johnwargo.com](https://johnwargo.com), implemented using [Eleventy](https://www.11ty.dev/).",
  "author": "John M. Wargo",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johnwargo/johnwargo-static-11ty"
  },
  "bugs": {
    "url": "https://github.com/johnwargo/johnwargo-static-11ty/issues"
  },
  "scripts": {
    "clean": "rimraf _site",
    "build:eleventy": "eleventy",
    "watch:eleventy": "eleventy --serve --quiet",
    "start": "npm-run-all --sequential clean watch:eleventy",
    "build": "npm-run-all --sequential clean build:eleventy"
  },
  "dependencies": {
    "@11ty/eleventy": "^3.0.0",
    "@11ty/eleventy-img": "^6.0.2",
    "@11ty/eleventy-plugin-rss": "^2.0.3",
    "@11ty/eleventy-plugin-syntaxhighlight": "^5.0.0",
    "eleventy-generate-category-pages": "^0.1.4",
    "eleventy-plugin-date": "^1.0.0",
    "eleventy-plugin-file-list": "^0.0.5",
    "eleventy-plugin-github-repos": "^0.0.2",
    "eleventy-plugin-post-stats": "^0.2.13",
    "eleventy-plugin-youtube-embed": "^1.13.0",
    "execa": "^9.5.2",
    "generate-build-info": "^0.0.3",
    "html-minifier": "^4.0.0",
    "markdown-it": "^14.1.0",
    "markdown-it-attrs": "^4.3.1",
    "npm-run-all": "^4.1.5",
    "outdent": "^0.8.0",
    "rimraf": "^6.0.1",
    "rss-parser": "^3.13.0",
    "sharp": "^0.34.1"
  },
  "keywords": [
    "eleventy",
    "plugin"
  ]
}